{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@routes/*": ["src/routes/*"],
      "@views/*": ["src/views/*"],
      "@types/*": ["src/types/*"],
      "@common/*": ["src/common/*"],
      "@enums/*": ["src/common/enums/*"],
      "@components/*": ["src/components/*"]
    }
  }
}
